@using System.Web.Optimization
<!DOCTYPE html>
<html>
<head>
    <meta name="viewport" content="width=device-width"/>
    <title>TicketSellingSystem</title>
    <base href="/"/>
    @Styles.Render("~/Content/bootstrap")
    @Styles.Render("~/Content/css")
</head>
<body>
<div class="wrapper">
    <div class="container-padding">
        @RenderBody()
        <div class="footer navbar-fixed-bottom">
            <div class="text-footer">
                Ticket Selling System. @DateTime.Now.Year
            </div>
        </div>
    </div>
</div>
@Scripts.Render("~/bundles/jquery")
@Scripts.Render("~/bundles/bootstrap")
@Scripts.Render("~/bundles/angular")
@Scripts.Render("~/bundles/application")
@Scripts.Render("~/bundles/events")
@Scripts.Render("~/bundles/services")
@Scripts.Render("~/bundles/controllers")
@Scripts.Render("~/bundles/constants")
@Scripts.Render("~/bundles/otherLibraries")
@RenderSection("scripts", false)
</body>
</html>